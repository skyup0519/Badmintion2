<?xml version="1.0" encoding="utf-8"?>
<inset
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:insetTop="1dip"
	android:insetBottom="1dip"
	android:insetLeft="1dip"
	android:insetRight="1dip">
	<shape>
		<size
			android:height="36dip"
			android:width="36dip" />
		<gradient
			android:startColor="#FF9FB3CC"
			android:centerColor="#FF597EAA"
			android:endColor="#FF5B80AB"
			android:angle="270" />
		<stroke
			android:width="2dip"
			android:color="#FFFB7A00" />
		<corners
			android:radius="5dp" />
		<padding
			android:top="5dip"
			android:bottom="5dip"
			android:left="8dip"
			android:right="8dip" />
	</shape>
</inset>
